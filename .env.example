# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Bot API Port (optional, defaults to 4000)
BOT_API_PORT=4000

# MongoDB Connection String
MONGODB_URI=your_mongodb_connection_string_here

# Intake Channel ID - The channel where the intake form message will be posted
INTAKE_CHANNEL_ID=your_channel_id_here

# Google Apps Script URL for form submission to Google Sheets
GOOGLE_APPS_SCRIPT_URL=your_google_apps_script_web_app_url_here

# Google Apps Script timeout in milliseconds (optional, defaults to 10000)
GOOGLE_APPS_SCRIPT_TIMEOUT=10000

# Google Apps Script retry attempts (optional, defaults to 3)
GOOGLE_APPS_SCRIPT_RETRIES=3

# Example channel ID format: 123456789012345678
